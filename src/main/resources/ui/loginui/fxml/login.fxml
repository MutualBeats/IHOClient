<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="presentation.loginui.LoginController">
   <children>
      <Button fx:id="signin" layoutX="485.0" layoutY="85.0" mnemonicParsing="false" onAction="#signin" prefHeight="147.0" prefWidth="166.0" />
      <Button fx:id="login" layoutX="600.0" layoutY="250.0" mnemonicParsing="false" onAction="#login" prefHeight="155.0" prefWidth="155.0" />
      <Button fx:id="exit" layoutX="555.0" layoutY="450.0" mnemonicParsing="false" onAction="#exit" prefHeight="123.0" prefWidth="135.0" />
      <Button fx:id="visit" layoutX="375.0" layoutY="480.0" mnemonicParsing="false" onAction="#visit" prefHeight="107.0" prefWidth="110.0" />
      <TextField fx:id="user_name" layoutX="285.0" layoutY="260.0" onMouseClicked="#userNameModify" prefHeight="30.0" prefWidth="210.0" />
      <PasswordField fx:id="password" layoutX="285.0" layoutY="327.0" onMouseClicked="#passwordModify" prefHeight="30.0" prefWidth="210.0" />
      <Label fx:id="name_warning" layoutX="285.0" layoutY="300.0" prefHeight="17.0" prefWidth="260.0" textFill="#ea8585">
         <font>
            <Font size="15.0" />
         </font></Label>
      <Label fx:id="pass_warning" layoutX="285.0" layoutY="370.0" prefHeight="17.0" prefWidth="260.0" textFill="#ea8585">
         <font>
            <Font size="15.0" />
         </font></Label>
   </children>
</AnchorPane>
