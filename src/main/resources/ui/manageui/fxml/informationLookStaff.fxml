<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.media.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="600.0"
	prefWidth="800.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="presentation.manageui.check.StaffInformationController">
	<children>
		<Pane fx:id="pane" layoutX="180.0" layoutY="100.0" prefHeight="460.0"
			prefWidth="580.0">
			<children>
				<Text layoutX="78.0" layoutY="41.0" strokeType="OUTSIDE"
					strokeWidth="0.0" text="人员信息" wrappingWidth="70.0">
					<font>
						<Font size="16.0" />
					</font>
				</Text>
				<Text layoutX="80.0" layoutY="107.0" strokeType="OUTSIDE"
					strokeWidth="0.0" text="用户名：" wrappingWidth="53.0" />
				<Text layoutX="80.0" layoutY="162.0" strokeType="OUTSIDE"
					strokeWidth="0.0" text="姓名：" />
				<Text layoutX="80.0" layoutY="217.0" strokeType="OUTSIDE"
					strokeWidth="0.0" text="联系方式：" />
				<Text layoutX="80.0" layoutY="272.0" strokeType="OUTSIDE"
					strokeWidth="0.0" text="酒店ID：" />
				<TextField fx:id="name" editable="false" layoutX="165.0"
					layoutY="144.0" onMouseClicked="#nameModify" prefHeight="16.0"
					prefWidth="170.0" />
				<TextField fx:id="phone" editable="false" layoutX="165.0"
					layoutY="201.0" onMouseClicked="#phoneModify" prefHeight="16.0"
					prefWidth="170.0" />
				<Button fx:id="cancel" layoutX="325.0" layoutY="379.0"
					mnemonicParsing="false" onAction="#cancel" prefHeight="26.0"
					prefWidth="60.0" />
				<Button fx:id="change" layoutX="145.0" layoutY="379.0"
					mnemonicParsing="false" onAction="#change" prefHeight="26.0"
					prefWidth="60.0" />
				<Button fx:id="confirm" disable="true" layoutX="145.0"
					layoutY="379.0" mnemonicParsing="false" onAction="#confirm"
					prefHeight="26.0" prefWidth="65.0" visible="false" />
				<Label fx:id="name_warning" alignment="TOP_LEFT" layoutX="350.0"
					layoutY="144.0" prefHeight="50.0" prefWidth="135.0" textFill="#e40c0c"
					wrapText="true" />
				<Label fx:id="phone_warning" alignment="TOP_LEFT" layoutX="350.0"
					layoutY="201.0" prefHeight="50.0" prefWidth="135.0" textFill="#e40c0c"
					wrapText="true" />
				<Text layoutX="80.0" layoutY="325.0" strokeType="OUTSIDE"
					strokeWidth="0.0" text="人员类型：" />
				<Label fx:id="user_name" layoutX="165.0" layoutY="89.0"
					prefHeight="25.0" prefWidth="170.0" />
				<Label fx:id="hotel_id" layoutX="165.0" layoutY="254.0"
					prefHeight="25.0" prefWidth="170.0" />
				<Label fx:id="type" layoutX="165.0" layoutY="307.0"
					prefHeight="25.0" prefWidth="170.0" />
			</children>
		</Pane>
		<Button fx:id="peopleInfo" layoutX="87.0" layoutY="175.0"
			mnemonicParsing="false" onAction="#peopleInfo" prefHeight="60.0"
			prefWidth="80.0" />
		<Button fx:id="addpeople" layoutX="5.0" layoutY="220.0"
			mnemonicParsing="false" onAction="#add_people" prefHeight="60.0"
			prefWidth="80.0" />
		<Button fx:id="addhotel" layoutX="90.0" layoutY="340.0"
			mnemonicParsing="false" onAction="#addhotel" prefHeight="60.0"
			prefWidth="80.0" />
		<Button fx:id="change_button" layoutX="5.0" layoutY="290.0"
			mnemonicParsing="false" onAction="#on_change" prefHeight="60.0"
			prefWidth="80.0" />
		<Button fx:id="check" layoutX="95.0" layoutY="253.0"
			mnemonicParsing="false" prefHeight="60.0" prefWidth="80.0" />
	</children>
</AnchorPane>
